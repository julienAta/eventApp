<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Debug View</title>
  </head>
  <body>
    <h1>Debug View</h1>
    <h2>Users</h2>
    <ul>
      <% users.forEach(user => { %>
      <li><%= user.id %> - <%= user.name %></li>
      <% }) %>
    </ul>
    <h2>Events</h2>
    <ul>
      <% events.forEach(event => { %>
      <li><%= event.id %> - <%= event.title %></li>
      <% }) %>
    </ul>
  </body>
</html>
